library(cells) {
  cell(BUF) {
    area: 4;
    pin(A) { direction: input; }
    pin(Y) { direction: output; function: "A"; }
  }
  cell(NOT) {
    area: 2;
    pin(A) { direction: input; }
    pin(Y) { direction: output; function: "!A"; }
   }
  cell(NAND2) {
      area: 4;
      pin(A1) { direction: input; }
      pin(A2) { direction: input; }
      pin(Y) { direction: output; function: "!(A1&A2)"; }
  }
  cell(NAND3) {
      area: 6;
      pin(A) { direction: input; }
      pin(B) { direction: input; }
      pin(C) { direction: input; }
      pin(Y) { direction: output; function: "!(A&B&C)"; }
  }
  cell(NOR2) {
      area: 4;
      pin(A1) { direction: input; }
      pin(A2) { direction: input; }
      pin(Y) { direction: output; function: "!(A1|A2)"; }
  }
  cell(NOR3) {
      area: 6;
      pin(A) { direction: input; }
      pin(B) { direction: input; }
      pin(C) { direction: input; }
      pin(Y) { direction: output; function: "!(A|B|C)"; }
  }
  cell(AOI21) {
      area: 6;
      pin(A) { direction: input; }
      pin(B1) { direction: input; }
      pin(B2) { direction: input; }
      pin(Y) { direction: output; function: "!(A|(B1&B2))"; }
  }
  cell(OAI21) {
      area: 6;
      pin(A) { direction: input; }
      pin(B1) { direction: input; }
      pin(B2) { direction: input; }
      pin(Y) { direction: output; function: "!(A&(B1|B2))"; }
  }
  cell(AOI22) {
      area: 8;
      pin(A1) { direction: input; }
      pin(A2) { direction: input; }
      pin(B1) { direction: input; }
      pin(B2) { direction: input; }
      pin(Y) { direction: output; function: "!((A1&A2)|(B1&B2))"; }
  }
  cell(OAI22) {
      area: 8;
      pin(A1) { direction: input; }
      pin(A2) { direction: input; }
      pin(B1) { direction: input; }
      pin(B2) { direction: input; }
      pin(Y) { direction: output; function: "!((A1|A2)&(B1|B2))"; }
  }
  cell(AOI211) {
      area: 8;
      pin(A) { direction: input; }
      pin(B) { direction: input; }
      pin(C1) { direction: input; }
      pin(C2) { direction: input; }
      pin(Y) { direction: output; function: "!(A|B|(C1&C2))"; }
  }
  cell(OAI211) {
      area: 8;
      pin(A) { direction: input; }
      pin(B) { direction: input; }
      pin(C1) { direction: input; }
      pin(C2) { direction: input; }
      pin(Y) { direction: output; function: "!(A&B&(C1|C2))"; }
  }
  cell(DFF_PP0) {
        area: 16;
		ff(IQ, IQN) {
			clocked_on: "C";
			next_state: "D";
			clear: "R";
		}
		pin(D) { direction: input; }
		pin(R) { direction: input; }
		pin(C) { direction: input; clock: true; }
		pin(Q) { direction: output; function: "IQ"; }
	}
}
